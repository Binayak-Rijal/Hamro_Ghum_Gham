!/frontend/public/
!/frontend/package.json
!/frontend/package-lock.json
!/frontend/index.html
!/frontend/vite.config.js
!/frontend/README.md
!/frontend/eslint.config.js
!/frontend/.gitignore

# Also ignore common nested vendor dirs under frontend
